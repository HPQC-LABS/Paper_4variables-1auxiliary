\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & =\alpha_{1234}\left(b_{3}b_{4}-b_{2}b_{3}b_{4}-b_{1}b_{3}b_{4}+b_{1}b_{2}b_{3}b_{4}\right)+\alpha_{123}\left(b_{3}-b_{1}b_{3}-b_{2}b_{3}+b_{1}b_{2}b_{3}\right)+\alpha_{124}\left(b_{4}-b_{1}b_{4}-b_{2}b_{4}+b_{1}b_{2}b_{4}\right)\\
