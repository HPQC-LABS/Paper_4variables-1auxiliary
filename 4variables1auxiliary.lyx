\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\alpha_{1234}\bar{b}_{1}\bar{b}_{2}b_{3}b_{4}+\alpha_{123}\bar{b}_{1}\bar{b}_{2}b_{3}+\alpha_{124}\bar{b}_{1}\bar{b}_{2}b_{4}+\alpha_{134}\bar{b}_{1}b_{3}b_{4}+\alpha_{234}\bar{b}_{2}b_{3}b_{4}
\end{equation}

\end_inset


